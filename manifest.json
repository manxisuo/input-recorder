{
	"manifest_version": 3,

	"name": "__MSG_extName__",
	"description": "__MSG_extDescription__",
	"version": "1.1.0",
	"default_locale": "en",
	
	"icons": 
	{ 
		"16": "img/16.png",             
		"48": "img/48.png",            
		"128": "img/128.png" 
	},
	
	"action": {
		"default_icon": "img/48.png",
		"default_popup": "popup.html"
	},
	
	"permissions": [
		"activeTab",
		"scripting",
		"storage"
	],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	}
}
